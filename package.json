{
  "name": "luckdb",
  "version": "0.1.0",
  "private": true,
  "description": "LuckDB - Modern, AI-powered database management platform",
  "keywords": [
    "database",
    "ai",
    "collaboration",
    "no-code",
    "low-code"
  ],
  "author": "LuckDB Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "pnpm --filter @luckdb/web dev",
    "dev:server": "cd server && make dev",
    "dev:all": "concurrently \"pnpm dev:server\" \"pnpm dev:web\"",
    "build": "turbo run build",
    "build:web": "pnpm --filter @luckdb/web build",
    "build:server": "cd server && make build",
    "test": "turbo run test",
    "test:server": "cd server && make test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "clean:all": "pnpm clean && cd server && make clean",
    "prepare": "husky install",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.14.0"
}

