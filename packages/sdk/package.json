{
  "name": "@easyspace/luckdb-sdk",
  "version": "1.0.7",
  "description": "TypeScript SDK for LuckDB - A collaborative database platform",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "pnpm build",
    "prepack": "pnpm build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docs": "typedoc src/index.ts",
    "basic": "ts-node --transpile-only examples/basic-usage.ts",
    "advanced": "ts-node --transpile-only examples/advanced-queries.ts",
    "test:setup": "tsx examples/00-setup-test-user.ts",
    "test:auth": "tsx examples/01-auth-test.ts",
    "test:validation": "tsx examples/01-sdk-basic-validation.ts",
    "test:formula": "tsx examples/02-formula-field-validation.ts",
    "test:space": "tsx examples/02-space-crud.ts",
    "test:record": "tsx examples/04-record-operations.ts",
    "test:view": "tsx examples/05-view-management.ts",
    "test:websocket": "tsx examples/10-websocket-realtime-updates.ts",
    "test:subscription": "tsx examples/05-websocket-subscription.ts",
    "test:websocket-debug": "tsx examples/06-websocket-debug.ts",
    "test:websocket-detailed": "tsx examples/07-websocket-detailed-test.ts",
    "test:performance": "tsx examples/97-performance-tests.ts",
    "test:destructive": "tsx examples/98-destructive-tests.ts",
    "test:comprehensive": "tsx examples/99-comprehensive-test.ts",
    "test:all": "pnpm test:setup && pnpm test:auth && pnpm test:space && pnpm test:record && pnpm test:view && pnpm test:comprehensive && pnpm test:destructive && pnpm test:performance"
  },
  "keywords": [
    "luckdb",
    "database",
    "collaboration",
    "airtable",
    "sdk",
    "typescript"
  ],
  "author": "LuckDB Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.17",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.24.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.12.2"
  },
  "optionalDependencies": {
    "ws": "^8.18.3"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/easyspace-ai/luckdb.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/easyspace-ai/luckdb/issues"
  },
  "homepage": "https://github.com/easyspace-ai/luckdb/tree/main/packages/sdk#readme",
  "packageManager": "pnpm@9.13.0"
}
