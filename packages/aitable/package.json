{
  "name": "@luckdb/aitable",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Next-generation Airtable-like component for LuckDB (Refactored Version)",
  "homepage": "https://github.com/luckdb/luckdb",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "source": "./src/index.ts",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    },
    "./dist/index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css",
      "default": "./dist/index.css"
    },
    "./store": {
      "types": "./dist/store/index.d.ts",
      "import": "./dist/store/index.js",
      "require": "./dist/store/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.js"
    },
    "./grid/error-handling": {
      "types": "./dist/grid/error-handling/index.d.ts",
      "import": "./dist/grid/error-handling/index.js",
      "require": "./dist/grid/error-handling/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "kanban"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:tsc": "cross-env NODE_ENV=production tsc",
    "build:strict": "cross-env NODE_ENV=production tsc --project tsconfig.strict.json",
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo ./tsconfig.build.tsbuildinfo ./node_modules/.cache ./.eslintcache",
    "dev": "rollup -c --watch",
    "dev:tsc": "tsc --build tsconfig.build.json --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "dev:examples": "vite --config examples/vite.config.ts",
    "build:examples": "vite build --config examples/vite.config.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "lint:strict": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --config .eslintrc.strict.cjs",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "typecheck:strict": "tsc --project ./tsconfig.strict.json --noEmit",
    "migrate:strict": "tsx scripts/migrate-to-strict.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test-cover": "vitest run --coverage",
    "refactor:week1": "npm run migrate:strict && npm run typecheck:strict && npm run lint:strict",
    "refactor:check": "npm run typecheck:strict && npm run lint:strict && npm run test"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "date-fns": "^4.1.0",
    "immer": "^10.0.4",
    "lodash": "4.17.21",
    "lru-cache": "^10.2.0",
    "lucide-react": "^0.545.0"
  },
  "peerDependencies": {
    "@luckdb/sdk": "workspace:*",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-table": "^8.11.7",
    "@tanstack/react-virtual": "^3.2.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-day-picker": "^9.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "tailwindcss": "^3.0.0",
    "zustand": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@luckdb/sdk": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    }
  },
  "devDependencies": {
    "@luckdb/sdk": "workspace:*",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-table": "^8.11.7",
    "@tanstack/react-virtual": "^3.2.0",
    "@testing-library/react": "^14.0.0",
    "@types/lodash": "4.17.0",
    "@types/node": "20.9.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-hammerjs": "1.0.7",
    "@types/sharedb": "^3.3.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "autoprefixer": "^10.4.21",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.5.6",
    "prettier": "3.2.5",
    "react": "^18.0.0",
    "react-day-picker": "^9.5.1",
    "react-dom": "^18.0.0",
    "rimraf": "5.0.5",
    "rollup": "^4.9.6",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.7.0",
    "typescript": "5.4.3",
    "vite": "^5.0.0",
    "vitest": "2.1.5",
    "zustand": "4.5.2"
  }
}